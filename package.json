    "autoprefixer": "^10.3.3",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.1.0",
    "bootswatch": "^5.1.3",
    "boxicons": "^2.1.1",
    "css-loader": "^7.1.0",
    "dotenv-webpack": "^7.0.3",
    "fast-glob": "^3.2.7",

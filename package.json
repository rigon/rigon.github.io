{
  "name": "homepage",
  "version": "1.0.1",
  "description": "Rigon's personal webpage",
  "main": "index.js",
  "scripts": {
    "build": "npm run static && npm run wpbuild",
    "dev": "webpack --config webpack.config.js --mode development --watch",
    "pack": "npm run build && cd _site && tar zcvf ../files.tar.gz *",
    "images": "node ./bin/images.js",
